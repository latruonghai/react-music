{
  "printWidth": 80,
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "never",
  "quoteProps": "preserve",
  "react/jsx-max-props-per-line": [1, { "when": "always" }]
}
